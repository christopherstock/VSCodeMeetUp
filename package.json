{
    "name": "ant-design-primer",
    "version": "1.0.0",
    "description": "Visual Studio Code MeetUp",
    "author": "Christopher Stock <christopher.stock@mayflower.de>",
    "license": "MIT",
    "main": "dist/index.html",
    "homepage": "https://www.mayflower.de",
    "repository": "https://github.com/christopherstock/VSCodeMeetUp.git",
    "bugs": "https://github.com/christopherstock/VSCodeMeetUp/issues",
    "scripts": {
        "typescript:compile": "tsc",
        "webpack:dev": "webpack --mode=development --watch",
        "webpack:production": "webpack --mode=production --watch",
        "webpack:devserver": "webpack-dev-server --mode=development",
        "typedoc:generate": "typedoc --out doc --listInvalidSymbolLinks --name \"Visual Studio Code MeetUp, (c) 2020 Mayflower GmbH\" src/typescript",
        "eslint:check": "eslint \"src/**/*.{ts,tsx}\"",
        "test:jest": "jest",
        "test:jest-coverage": "jest --coverage"
    },
    "devDependencies": {
        "@types/axios": "0.14.0",
        "@types/chai": "4.1.7",
        "@types/enzyme": "3.10.3",
        "@types/jest": "25.1.2",
        "@types/moment": "2.13.0",
        "@types/moxios": "0.4.9",
        "@types/node": "9.6.48",
        "@types/react": "16.9.41",
        "@types/react-dom": "16.9.8",
        "@types/webpack": "4.4.29",
        "@typescript-eslint/eslint-plugin": "2.17.0",
        "@typescript-eslint/eslint-plugin-tslint": "2.17.0",
        "@typescript-eslint/parser": "2.17.0",
        "awesome-typescript-loader": "5.2.1",
        "chai": "4.2.0",
        "css-loader": "0.28.11",
        "enzyme": "3.10.0",
        "enzyme-adapter-react-16": "1.14.0",
        "eslint": "6.8.0",
        "eslint-plugin-import": "1.0",
        "eslint-plugin-prefer-arrow": "1.0",
        "ignore-styles": "5.0.1",
        "jest": "25.1.0",
        "less": "3.11.1",
        "less-loader": "4.1.0",
        "react-test-renderer": "16.10.2",
        "source-map-loader": "0.2.4",
        "style-loader": "0.20.3",
        "ts-jest": "24.1.0",
        "ts-node": "7.0.1",
        "typedoc": "0.16.9",
        "typescript": "3.7.5",
        "webpack": "4.41.6",
        "webpack-cli": "3.3.10",
        "webpack-dev-server": "3.10.3"
    },
    "dependencies": {
        "antd": "3.26.9",
        "axios": "0.19.2",
        "moment": "2.29.2",
        "moxios": "0.4.0",
        "rc-scroll-anim": "2.7.4",
        "rc-tween-one": "2.6.8",
        "react": "16.12.0",
        "react-dom": "16.12.0",
        "wowjs": "1.1.3"
    },
    "jest": {
        "verbose": true,
        "transform": {
            ".(ts|tsx)": "ts-jest"
        },
        "testRegex": "(/src/test/.*\\.(test|spec))\\.(ts|tsx|js|jsx)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx"
        ],
        "testURL": "http://localhost/",
        "setupFiles": [
            "<rootDir>/src/test/index.ts"
        ]
    }
}
